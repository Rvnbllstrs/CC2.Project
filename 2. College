 public static void main(String[] args) {
        Scanner sc = new Scanner (System.in); 
        System.out.println("What college you want to go?: ");
        String cas = sc.next();
        
        
        if(cas.equals("CAS")){
            System.out.println("What program are you in?");
            String cass = sc.next();
            
            if(cass.equals("Batchelor of Arts in English")){
                System.out.println("You are with CAS in a BAE program.");   
            }
            else if (cass.equals("Bachelor of Arts in Political Science")){
                System.out.println("You are with CAS in a BAE program.");  
            }
            else if (cass.equals("Bachelor of Arts in Communication")){
                System.out.println("You are with CAS in a BAC program.");  
            }
            else if (cass.equals("Bachelor of Science in Psychology")){
                System.out.println("You are with CAS in a BSP program.");  
            }
            else {
                System.out.println("Congratulations! You belong to CAS!");  
            }
        }
        
                
        
            else if(cas.equals("CBA")){
            System.out.println("What program are you in?");
            String cass = sc.next();
            
            if (cass.equals("Bachelor of Science in Business Administration")){
                System.out.println("You are with CAS in a BSBA program.");   
